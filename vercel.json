{
    "builds": [
      {
        "src": "emotion_recognition/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",
          "runtime": "python3.10",
          "memory": 1024,
          "timeout": 60
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "emotion_recognition/wsgi.py"
      }
    ]
  }