{
    "version": 2,
    "builds": [
      {
        "src": "emotion_recognition/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": 15,
          "runtime": "python3.10"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      
      {
        "src": "/(.*)",
        "dest": "emotion_recognition/wsgi.py"
      }
    ],
    "env": {
        "PYTHON_VERSION": "3.10"
      }
  }
  